96933fe2c84c37172b533d047411f5e6|file:///Users/louis/Dev/Projects/komi/komi-mobile/app/screens/waiting-room-screen/waiting-room-screen.tsx|125|1|V|typescriptreact
	    React.useEffect(() => {
	      if (booking && booking?.experience?.type !== CONTENT_TYPE.liveClass) return null
	      if (firebaseRefDBKey && booking && mode === APP_MODE.talent) {
	        firebaseService.onChange(firebaseRefDBKey, (snapshot) => {
	          const data = snapshot.val()
	          if (!data?.startedAt && moment().diff(moment(booking?.date), "minutes") > 10) {
	            notifications.show({
	              type: "error",
	              text1: translate("errors.notparticipateAfter10Mintues"),
	            })
	            navigation.goBack()
	          } else if (
	            data?.talentLastConnection &&
	            moment().diff(moment(data?.talentLastConnection), "minutes") > 5
	          ) {
	            notifications.show({
	              type: "error",
	              text1: translate("errors.cannotReconnect5Mintues"),
	            })
	            navigation.goBack()
	          }
	        })
	      }
	
	      if (firebaseRefDBKey && booking && mode === APP_MODE.consumer) {
	        firebaseService.onChange(firebaseRefDBKey, (snapshot) => {
	          const data = snapshot.val()
	          if (!data?.startedAt && moment().diff(moment(booking?.date), "minutes") > 10) {
	            if (!hasError) {
	              notifications.show({
	                type: "error",
	                text1: translate("errors.talentNotAccessLiveClassess"),
	              })
	              setHasError(true)
	              navigation.goBack()
	            }
	          } else if (
	            data?.talentLastConnection &&
	            moment().diff(moment(data?.talentLastConnection), "minutes") > 5
	          ) {
	            if (!hasError) {
	              notifications.show({
	                type: "error",
	                text1: translate("errors.talentNotRejoinLiveClassess"),
	              })
	              setHasError(true)
	              navigation.goBack()
	            }
	          }
	        })
	      }
	    }, [firebaseRefDBKey, booking, hasError, mode])
