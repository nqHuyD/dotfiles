	/* eslint-disable react-native/no-inline-styles */
	import React from "react"
	import { observer } from "mobx-react-lite"
	import { StyleSheet, View, ViewStyle, TouchableOpacity } from "react-native"
	import { Icon, Screen, Text, Avatar, Button } from "components"
	import {
	  ALIGN_CENTER,
	  color,
	  FULL_WIDTH,
	  JUSTIFY_CENTER,
	  metrics,
	  MT_2,
	  MT_5,
	  MT_6,
	  ROW,
	  spacing,
	} from "theme"
	import { useNavigation, RouteProp } from "@react-navigation/native"
	import { NavigationRoutes } from "navigation/navigation-routes"
	import { rgbColor } from "utils/color"
	import { Divider } from "react-native-elements"
	import { HomeStackParamList } from "navigation/types"
	
	interface SpecialRequestSentScreenProps {
	  route: RouteProp<HomeStackParamList, "SpecialRequestSentScreen">
	}
	
	export const SpecialRequestSentScreen = observer(({ route }: SpecialRequestSentScreenProps) => {
	  const navigation = useNavigation()
	
	  const { requestId, talent, budget } = route?.params
	
	  const _goToRequestDetail = React.useCallback(() => {
	    navigation.navigate(NavigationRoutes.ConsumerHistorySpecialRequestDetailScreen, {
	      bookingId: requestId,
	    })
	  }, [requestId])
	
	  const renderCard = React.useCallback(() => {
	    return (
	      <TouchableOpacity
	        style={{
	          width: "100%",
	          borderRadius: 16,
	          borderWidth: 1,
	          borderColor: color.themeColor.line,
	          marginTop: spacing[5],
	          paddingHorizontal: spacing[4],
	          paddingVertical: spacing[4],
	        }}
	        onPress={_goToRequestDetail}
	      >
	        <View
	          style={[
	            ROW,
	            {
	              justifyContent: "space-between",
	              alignItems: "center",
	              marginBottom: spacing[2],
	            },
	          ]}
	        >
	          <View>
	            <Text
	              tx="specialRequestSentScreen.requestId"
	              txOptions={{ id: requestId }}
	              preset="regularS"
	              style={{ color: rgbColor(color.themeColor.dark, 0.7) }}
	            />
	            <Text
	              tx="specialRequestSentScreen.watingForResponse"
	              preset="regularS"
	              style={{ color: color.themeColor.yellow }}
	            />
	          </View>
	          <View>
	            <Text text={`$${budget}`} preset="semiBoldM" />
	          </View>
	        </View>
	        <Divider
	          style={{
	            backgroundColor: color.themeColor.line,
	            marginBottom: spacing[2],
	            paddingHorizontal: spacing[4],
	          }}
	        />
	        <View style={[ROW, ALIGN_CENTER]}>
	          <Avatar
	            size={24}
	            title={`${talent?.firstName || ""} ${talent?.lastName || ""}`}
	            source={{ uri: talent?.avatar }}
	          />
	          <Text text="Als" style={{ marginLeft: spacing[2] }} preset="semiBoldR" />
	        </View>
	      </TouchableOpacity>
	    )
	  }, [talent, requestId, budget])
	
	  return (
	    <Screen
	      preset="scroll"
	      containerStyle={[styles.container, JUSTIFY_CENTER, ALIGN_CENTER] as ViewStyle}
	      header={{
	        rightIcon: "close",
	        onRightPress: () =>
	          navigation.reset({
	            index: 1,
	            routes: [{ name: NavigationRoutes.MainStack }],
	          }),
	        style: { borderBottomWidth: 0 },
	      }}
	    >
	      <View style={[JUSTIFY_CENTER, ALIGN_CENTER, { marginTop: metrics.screenHeight * 0.1 }]}>
	        <View style={{ position: "absolute", bottom: spacing[3] }}>
	          <Icon name="firework-request-sent" />
	        </View>
	        <Icon name="request-sent" />
	      </View>
	      <View style={MT_6}>
	        <Text tx="specialRequestSentScreen.requestSent" preset="semiBoldMP" />
	      </View>
	      <Text
	        tx="specialRequestSentScreen.weWillUpdateToYouWhenTalentResponse"
	        txOptions={{ name: "All" }}
	        style={[
	          MT_2,
	          {
	            color: rgbColor(color.themeColor.dark, 0.7),
	            textAlign: "center",
	            paddingHorizontal: spacing[7],
	          },
	        ]}
	      />
	      {renderCard()}
	      <Button
	        tx="specialRequestSentScreen.viewSpecialRequest"
	        containerStyle={[FULL_WIDTH, MT_5]}
	        onPress={_goToRequestDetail}
	      />
	    </Screen>
	  )
	})
	
	const styles = StyleSheet.create({
	  container: {
	    paddingHorizontal: spacing[4],
	  },
	})
